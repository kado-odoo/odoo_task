<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <!-- Inhetit Res Partner From View -->
    <record id="res_partner_form_inherit_product" model="ir.ui.view">
        <field name="name">res.partner</field>
        <field name="model">res.partner</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='internal_notes']" position="after">
                <page string="inventory" name="inventory">
                    <field name="partnerwise_umo_ids">
                        <tree editable="bottom">
                            <field name="product_uom_category" invisibal="1"/>
                            <!-- <field name="partner_id" widget="Many2one" invisible="1"/> -->
                            <field name="product_id" widget="Many2one"/>
                            <field name="product_uom_id" widget="Many2one"/>
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
